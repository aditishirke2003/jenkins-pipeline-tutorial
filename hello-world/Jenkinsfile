pipeline {
    agent any 
    stages {
        stage('Static Analysis') {
            steps {
                echo 'This is a new analysis of the code' 
            }
        }
        stage('Compile') {
            steps {
                echo 'Compile Aditi Shirke for this practical' 
            }
        }
        stage('Security Check') {
            steps {
                echo 'Run the security check against the application for this practical' 
            }
        }
        stage('Run Unit Tests') {
            steps {
                echo 'Run unit tests from the source code for this practical' 
            }
        }
        stage('Run Integration Tests') {
            steps {
                echo 'Run crucial integration tests from the source code for this practical' 
            }
        }
        stage('Publish Artifacts') {
            steps {
                echo 'Save the assemblies generated from the compilation for this practical' 
            }
        }
    }
}
